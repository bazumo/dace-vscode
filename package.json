{
    "name": "sdfv",
    "displayName": "DaCe SDFG Viewer (SDFV)",
    "description": "DaCe SDFG Viewer (SDFV) for VS Code",
    "version": "0.1.7",
    "engines": {
        "vscode": "^1.45.1"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCustomEditor:sdfgCustom.sdfv"
    ],
    "main": "./out/extension.js",
    "publisher": "phschaad",
    "bugs": {
        "url": "https://github.com/spcl/dace-vscode/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/spcl/dace-vscode"
    },
    "icon": "icon.png",
    "contributes": {
        "customEditors": [
            {
                "viewType": "sdfgCustom.sdfv",
                "displayName": "SDFG Viewer",
                "selector": [
                    {
                        "filenamePattern": "*.sdfg"
                    }
                ],
                "priority": "default"
            }
        ],
        "iconThemes": [
            {
                "id": "sdfgicon",
                "label": "Seti + SDFG Extension",
                "path": "./fileicons/sdfg-icon-theme.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^7.0.1",
        "@types/node": "^12.12.39",
        "@types/vscode": "^1.45.1",
        "@typescript-eslint/eslint-plugin": "^2.33.0",
        "@typescript-eslint/parser": "^2.33.0",
        "eslint": "^6.8.0",
        "glob": "^7.1.6",
        "mocha": "^7.1.2",
        "typescript": "^3.9.2",
        "vscode-test": "^1.3.0"
    },
    "dependencies": {}
}